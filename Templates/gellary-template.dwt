<script>//ẩn hiện audio khi rê chuột vào slideshow
	$(document).ready(function() {
  $( "audio" ).hide();
  $( ".wrapper" ).mousemove(function() {
    	$( "audio" ).show();
	});
	$(document).mouseout(function(){
			$("audio").hide();
		});
});
</script>
